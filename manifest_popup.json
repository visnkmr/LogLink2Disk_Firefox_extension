{
  "name": "LogLinktoDisk",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
      "scripts": ["dist/background.js"]
  },
  "browser_action": {
      "default_title": "LogLinktoDisk",
      "default_popup": "popup.html"
  },
  "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>"
  ],
  "author": "Vishnu N K",
  "applications": {
      "gecko": {
          "id": "loglinktodisk@example.com"
      }
  }
}
